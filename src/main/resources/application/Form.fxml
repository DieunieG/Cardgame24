<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main">
    <Label text="Enter Name:" layoutX="20" layoutY="20"/>
    <TextField fx:id="nameField" layoutX="100" layoutY="16" prefWidth="200"/>
    
    <Button text="Submit" layoutX="320" layoutY="16"/>
    
    <TableView fx:id="tableView" layoutX="20" layoutY="60" prefWidth="400" prefHeight="200">
        <columns>
            <TableColumn text="Name" fx:id="nameColumn" prefWidth="150"/>
            <TableColumn text="Age" fx:id="ageColumn" prefWidth="100"/>
        </columns>
    </TableView>
</AnchorPane>